использование System.Collections;
используя System.Collections.Generic;
с помощью UnityEngine;

привет общедоступного класса: MonoBehaviour
{
    // Старт вызывается перед обновлением первого кадра
    пустое начало ()
    {
        //Вывод в консоль привествия
        print("здрасте негры земляне мы вас любим");
    }

    // Обновление вызывается один раз за кадр
    пустое обновление ()
    {
        print("здрасте негры земляне мы вас любим");
     }
}
